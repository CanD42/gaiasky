<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='com.badlogic.gdx.controllers.controllers-gwt' />

	<inherits name='GaiaSandbox' />
	<inherits name='postprocessing' />
	<inherits name="com.google.gwt.http.HTTP" />
	<entry-point class='gaia.cu9.ari.gaiaorbit.client.GaiaSandboxWebGL' />
	
	<set-configuration-property name="gdx.assetpath" value="../android/assets" />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.interfce.FullGui" />
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.stars.HYGBinaryLoader" />
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.stars.OctreeCatalogLoader" />
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.JsonLoader" />
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.constel.ConstellationsLoader" />
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.constel.ConstelBoundariesLoader" />
	
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.orbit.OrbitalParametersProvider" />
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.orbit.OrbitFileDataProvider" />
	<extend-configuration-property name="gdx.reflect.include" value="gaia.cu9.ari.gaiaorbit.data.orbit.OrbitSamplerDataProvider" />
	
</module>
